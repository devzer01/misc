class Hb_App_Proposal_ProposalAuctionComment extends Hb_App_Object
{

	/**
	 * Proposal auction comment
	 *
	 * @var string
	 **/
	protected $comment = null;

	/**
	 * Proposal auction comment Id
	 *
	 * @var int
	 **/	
	protected $proposal_auction_comment_id = null;
	
	/**
	 * Proposal auction comment type Id
	 *
	 * @var int
	 **/
	protected $proposal_auction_comment_type_id = null;
	
	/**
	 * Proposal auction Id
	 *
	 * @var int
	 **/
	protected $proposal_auction_id = null;
	
	/**
	 * Proposal auction comment subject
	 *
	 * @var string
	 **/
	protected $subject = null;

	/**
	 * Create ProposalAuctionComment Object
	 *
	 * @param int $id Proposal auction comment id
	 * @param int $proposal_auction_comment_type_id Proposal auction comment type Id
	 * @param int $proposal_auction_id Proposal auction Id 
	 * @param string $comment Proposal auction comment
	 * @param int $subject Proposal auction subject  
	 */	
	function __construct($id = null,
				  				$proposal_auction_comment_type_id = null,
				  				$proposal_auction_id = null,
				  				$comment = null,
				  				$subject = null )
	{
		$this->proposal_auction_comment_id = $id;
		$this->proposal_auction_comment_type_id = $proposal_auction_comment_type_id;
		$this->proposal_auction_id = $proposal_auction_id;		
		$this->comment = $comment;
		$this->subject = $subject;
		parent::__construct($id);
	}

	/**
	* Returns the Proposal Auction Comment
 	* 
	* @return string The Proposal auction comment
	**/
	public function GetComment()
	{
		return $this->comment;
	}

	/**
	* Returns the Proposal Auction Comment Id
 	* 
	* @return int The Proposal auction comment Id
	**/
	public function GetProposalAuctionCommentId()
	{
		return $this->proposal_auction_comment_id;
	}

	/**
	* Returns the Proposal Auction Comment type Id
 	* 
	* @return int The Proposal Auction Comment type Id
	**/
	public function GetProposalAuctionCommentTypeId()
	{
		return $this->proposal_auction_comment_type_id;
	}

	/**
	* Returns the Proposal Auction Id
 	* 
	* @return int The Proposal Auction Id
	**/
	public function GetProposalAuctionId()
	{
		return $this->proposal_auction_id;
	}

	/**
	* Returns the Proposal Auction comment subject
 	* 
	* @return int The Proposal Auction comment subject
	**/
	public function GetSubject()
	{
		return $this->subject;
	}

	/**
	 * Sets the Proposal auction comment
 	 * 
	 * @param string $comment Proposal auction comment
	 */
	public function SetComment($comment)
	{
		$this->comment = $comment;
		$this->MarkDirty();
	}

	/**
	 * Sets the Proposal auction comment Id
 	 * 
	 * @param int $proposal_auction_comment_id Proposal auction comment Id
	 */
	public function SetProposalAuctionCommentId($proposal_auction_comment_id)
	{
		$this->proposal_auction_comment_id = (int) $proposal_auction_comment_id;
		$this->__id = (int) $proposal_auction_comment_id;		
		$this->MarkDirty();
	}

	/**
	 * Sets the Proposal auction comment type Id
 	 * 
	 * @param int $proposal_auction_comment_type_id Proposal auction comment type Id
	 */
	public function SetProposalAuctionCommentTypeId($proposal_auction_comment_type_id)
	{
		$this->proposal_auction_comment_type_id = (int) $proposal_auction_comment_type_id;
		$this->MarkDirty();
	}

	/**
	 * Sets the Proposal auction Id
 	 * 
	 * @param int $proposal_auction_id Proposal auction Id
	 */
	public function SetProposalAuctionId($proposal_auction_id)
	{
		$this->proposal_auction_id = (int) $proposal_auction_id;
		$this->MarkDirty();		
	}

	/**
	 * Sets the Proposal auction comment subject
 	 * 
	 * @param string $subject Proposal auction comment subject
	 */
	public function SetSubject($subject)
	{
		$this->subject = $subject;
		$this->MarkDirty();
	}

}
?>