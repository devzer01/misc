class Hb_App_Proposal_ProposalRevisionStudyClassification extends Hb_App_Object 
{
	/**
	 * proposal revision study classification id
	 *
	 * @var int 
	 */
	private $proposal_revision_study_classification_id = null;

	/**
	 * Proposal revison id
	 *
	 * @var int 
	 */
	private $proposal_revision_id = null;
	
	/**
	 * Study Category Type id 
	 *
	 * @var int 
	 */
	private $study_category_type_id = null;
	
	
	public function __construct($id 								= null,
										 $proposal_revision_id 		= null,
										 $study_category_type_id	= null)
	{		
		$this->proposal_revision_study_classification_id	= $id;
		$this->proposal_revision_id								= $proposal_revision_id;
		$this->study_category_type_id								= $study_category_type_id;
		
		parent::__construct($id);
	}
	
	/**
	 * Set the Proposal Revision Study Classification ID for the  Proposal Revision Study Classification
	 * 
	 * @param int $proposal_revision_study_classification_id Proposal Revision Study Classification ID
	 */
	public function SetProposalRevisionStudyClassificaitonId($proposal_revision_study_classification_id)
	{
		$this->proposal_revision_study_classification_id	= $proposal_revision_study_classification_id;
		$this->__id														= $proposal_revision_study_classification_id;
		$this->MarkDirty();
	}
	
	/**
	 * Set the Proposal Revision ID for the  Proposal Revision Study Classification
	 * 
	 * @param int $proposal_revision_id Proposal Revision ID
	 */
	public function SetProposalRevisionID($proposal_revision_id)
	{
		$this->proposal_revision_id	= $proposal_revision_id;
		$this->MarkDirty();
	}
	
	/**
	 * Set the Study Category Type ID for the  Proposal Revision Study Classification
	 * 
	 * @param int $study_category_type_id Study Category Type ID
	 */
	public function SetStudyCategoryTypeID($study_category_type_id)
	{
		$this->study_category_type_id	= $study_category_type_id;
		$this->MarkDirty();
	}
	
	/**
	 * Return the Proposal Revision Study Classfication ID for the Proposal Revision Study Classification
	 *
	 * @return int Proposal Revision Study Classfication ID
	 */
	public function GetProposalRevisionStudyClassificationID()
	{
		return $this->proposal_revision_study_classification_id;
	}
	
	/**
	 * Return the Proposal Revision ID for the Proposal Revision Study Classification
	 *
	 * @return int Proposal Revision ID
	 */
	public function GetProposalRevisionID()
	{
		return $this->proposal_revision_id;
	}
	
	/**
	 * Return the Study Category Type ID for the Proposal Revision Study Classification
	 *
	 * @return int Study Category Type ID
	 */
	public function GetStudyCategoryTypeID()
	{
		return $this->study_category_type_id;
	}
}
