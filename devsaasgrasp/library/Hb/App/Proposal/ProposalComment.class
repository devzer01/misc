		$this->proposal_comment_id = $id;
		$this->proposal_id = $proposal_id;
		$this->proposal_comment_type_id = $proposal_comment_type_id;
		$this->proposal_status_id = $proposal_status_id;
		$this->proposal_comment_text = $proposal_comment_text;
		$this->proposal_comment_date = $proposal_comment_date;
		$this->login = $login;
		$this->user_id = $user_id;		
		
		parent::__construct($id);
	}


	/**
	 * Return proposal comment id
	 *
	 * @author jsadique
	 * @return int Proposal comment id
	 */
	public function GetProposalCommentId()
