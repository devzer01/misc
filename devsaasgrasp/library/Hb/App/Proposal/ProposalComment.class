	 * @param string $proposal_comment_text Comment text
	 * @param int $proposal_comment_date Comment date
	 * @param int $login Login id
	 * @param int $user_id User id
	 */		
	function __construct(	$id = null,
									$proposal_id = null,
									$proposal_comment_type_id = null,
									$proposal_status_id = null,
									$proposal_comment_text = null,
									$proposal_comment_date = null,
									$login = null,
									$user_id = null															
