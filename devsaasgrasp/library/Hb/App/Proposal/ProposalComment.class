	 */
	public function SetLogin($login)
	{
		$this->login = (int) $login;
		$this->MarkDirty();
	}

	/**
	 * Sets proposal comment date
	 * 
	 * @author jsadique
	 * @param string $comment_date Comment date
	 */
	public function SetProposalCommentDate($comment_date)
	{
		$this->proposal_comment_date = $comment_date;
		$this->MarkDirty();
	}

	/**
	 * Sets proposal comment id
	 * 
	 * @author jsadique
	 * @param int $id Comment Id
	 */
	public function SetProposalCommentId($id)
	{
		$this->proposal_comment_id = (int) $id;
