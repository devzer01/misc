	/**
	 * Sets Access Key
	 * 
	 * @param string $access_key Access Key
	 */
	public function SetAccessKey($access_key)
	{
		$this->access_key = $access_key;
		
		$this->MarkDirty();
	}

	/**
	 * Sets Account Contact Id
	 * 
	 * @param int $account_contact_id Account Contact Id
	 */
	public function SetAccountContactId($account_contact_id)
	{
		$this->account_contact_id = (int)$account_contact_id;
		
		$this->MarkDirty();
	}

	/**
	 * Sets Contact Email
	 * 
	 * @param string $contact_email Contact Email
	 */
	public function SetContactEmail($contact_email)
	{
		$this->contact_email = $contact_email;
		
		$this->MarkDirty();
	}

	/**
	 * Sets ContactFirstName
	 * 
	 * @param string $contact_first_name ContactFirstName
	 */
	public function SetContactFirstName($contact_first_name)
	{
		$this->contact_first_name = $contact_first_name;
		
		$this->MarkDirty();
	}

	/**
	 * Sets Contact Last Name
	 * 
	 * @param string $contact_last_name Last Name
	 */
	public function SetContactLastName($contact_last_name)
	{
		$this->contact_last_name = $contact_last_name;
		
		$this->MarkDirty();
	}

	/**
	 * Sets Invite Sent Date
	 * 
	 * @param string $invite_sent_date Invite Sent Date
	 */
	public function SetInviteSentDate($invite_sent_date)
	{
		$this->invite_sent_date = $invite_sent_date;
		
		$this->MarkDirty();
	}

	/**
	 * Sets Proposal Auction Account Contact Id
	 * 
	 * @param int $proposal_auction_account_contact_id Proposal Auction Account Contact Id
	 */
	public function SetProposalAuctionAccountContactId($proposal_auction_account_contact_id)
	{
		$this->proposal_auction_account_contact_id = (int)$proposal_auction_account_contact_id;
		$this->__id = (int) $proposal_auction_account_contact_id;
		
		$this->MarkDirty();
	}

