	 * Sets Proposal Auction AccountId
	 * 
	 * @param int $proposal_auction_account_id Proposal Auction AccountId
	 */
	public function SetProposalAuctionAccountId($proposal_auction_account_id)
	{
		$this->proposal_auction_account_id = (int)$proposal_auction_account_id;
		
		$this->MarkDirty();
	}

	/**
	 * Sets Proposal Auction User Id
	 * 
	 * @param int $proposal_auction_user_id  Proposal Auction User Id
	 */
	public function SetProposalAuctionUserId($proposal_auction_user_id)
	{
		$this->proposal_auction_user_id = (int)$proposal_auction_user_id;
		
		$this->MarkDirty();
	}
}
