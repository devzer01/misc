class Hb_App_Proposal_ProposalRevisionService extends Hb_App_Object 
{

	/**
	 * Proposal Revision Id
	 *
	 * @var int 
	 */
	protected $proposal_revision_id = null;
	
	/**
	 * Proposal Revision Service Id
	 *
	 * @var int 
	 */
	protected $proposal_revision_service_id = null;
	
	/**
	 * Service Id
	 *
	 * @var int 
	 */
	protected $service_id = null;

	/**
	 * Create ProposalRevisionService Object
	 *
	 * @param int $id Proposal Revision Service Id
	 * @param int $proposal_revision_id Proposal Revision Id
	 * @param int $service_id Service Id
	 */
	function __construct($id 							= null,
								$proposal_revision_id 	= null,
								$service_id 				= null
								)
	{
		$this->proposal_revision_service_id = $id;
		$this->proposal_revision_id			= $proposal_revision_id;
		$this->service_id							= $service_id;
		
		parent::__construct($id);
	}

	/**
	 * Return the Proposal Revision Id for Proposal Revision Service
	 *
	 * @return int Proposal Revision Id
	 */
	public function GetProposalRevisionId()
	{
		return $this->proposal_revision_id;
	}

	/**
	 * Return the Proposal Revision Service Id for Proposal Revision Service
	 *
	 * @return int Proposal Revision Service Id
	 */
	public function GetProposalRevisionServiceId()
	{
		return $this->proposal_revision_service_id;
	}

	/**
	 * Return the Service Id for Proposal Revision Service
	 *
	 * @return int Service Id
	 */
	public function GetServiceId()
	{
		return $this->service_id;
	}

	/**
	 * Set the Proposal Revision Id for Proposal Revision Service
	 * 
	 * @param int $proposal_revision_id Proposal Revision Id
	 */
	public function SetProposalRevisionId($proposal_revision_id)
	{
		$this->proposal_revision_id = (int) $proposal_revision_id;
		$this->MarkDirty();
	}

	/**
	 * Set the Proposal Revision Service Id for Proposal Revision Service
	 * 
	 * @param int $proposal_revision_service_id Proposal Revision Service Id
	 */
	public function SetProposalRevisionServiceId($proposal_revision_service_id)
	{
		$this->proposal_revision_service_id = (int)$proposal_revision_service_id;
		$this->__id									= (int)$proposal_revision_service_id;
		$this->MarkDirty();
	}

	/**
	 * Set the Service Id for Proposal Revision Service
	 * 
	 * @param int $service_id Service Id
	 */
	public function SetServiceId($service_id)
	{
		$this->service_id = (int) $service_id;
		$this->MarkDirty();
	}
	

}
?>