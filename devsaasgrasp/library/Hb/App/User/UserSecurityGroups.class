class Hb_App_User_UserSecurityGroups extends Hb_App_Collection 
{
	/**
	 * Login
	 *
	 * @var int
	 */ 
	protected $login = null;
	
	/**
	 * Add User Security Groups
	 *
	 * @param Hb_App_User_UserSecurityGroup $security_group User Security group
	 */ 
	public function AddItem(Hb_App_User_UserSecurityGroup $security_group)
	{
		$this->login = $security_group->GetLogin();
		parent::AddItem($security_group);
	}
}
