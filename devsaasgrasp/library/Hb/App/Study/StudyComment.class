		$this->study_comment_id = $id;
		$this->study_id = $study_id;
		$this->department_id = $department_id;
		$this->comment_date = $comment_date;
		$this->comment_type_id = $comment_type_id;
		$this->alert_level_id = $alert_level_id;
		$this->notes = $notes;
		$this->comment_reported = $comment_reported;
		$this->comment_reported_date = $comment_reported_date;
		
		parent::__construct($id);
