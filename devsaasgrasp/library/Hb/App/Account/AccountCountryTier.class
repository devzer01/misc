class Hb_App_Account_AccountCountryTier extends Hb_App_Object
{
	/**
	 * Account Country Tier Id
	 *
	 * @var int
	 */
	protected $account_country_tier_id = null;
	
	/**
	 * Account Id
	 *
	 * @var int
	 */
	protected $account_id = null;
	
	/**
	 * Country Code
	 *
	 * @var string
	 */
	protected $country_code = null;
	
	/**
	 * Country Tier Id
	 *
	 * @var int
	 */
	protected $country_tier_id = null;

	/**
	 * Create AccountCountryTier Object
	 * 
	 * @param int $id Account country tier id
	 * @param int $account_id Account id
	 * @param string $country_code Country code
	 * @param int $country_tier_id Country tier id
	 */
	function __construct($id 					= null, 
								$account_id 		= null,
								$country_code 		= null,
								$country_tier_id 	= null)
	{
		$this->account_country_tier_id 	= $id;
		$this->account_id						= $account_id;
		$this->country_code 					= $country_code;
		$this->country_tier_id 				= $country_tier_id;
		
		parent::__construct($id);
	}

	/**
	 * Returns the Account Country Tier Id
	 *
	 * @return int Account Country Tier Id
	 */
	public function GetAccountCountryTierId()
	{
		return $this->account_country_tier_id;
	}

	/**
	 * Returns the Country Code
	 *
	 * @return string Country Code
	 */
	public function GetCountryCode()
	{
		return $this->country_code;
	}

	/**
	 * Returns the Country Tier Id
	 *
	 * @return int Country Tier Id
	 */
	public function GetCountryTierId()
	{
		return $this->country_tier_id;
	}

	/**
	 * Returns the Account Id
	 *
	 * @return int Account Id
	 */
	public function GetAccountId()
	{
		return $this->account_id;
	}

	/**
	 * Sets the Account Country Tier Id
	 * 
	 * @param int $account_country_tier_id Account Country Tier Id
	 */
	public function SetAccountCountryTierId($account_country_tier_id)
	{
		$this->account_country_tier_id = $account_country_tier_id;
		
		$this->__id = (int) $account_country_tier_id;

		$this->MarkDirty();
	}

	/**
	 * Sets the Country Code
	 * 
	 * @param string $country_code Country Code
	 */
	public function SetCountryCode($country_code)
	{
		$this->country_code = $country_code;

		$this->MarkDirty();
	}

	/**
	 * Sets the Country Tier Id
	 * 
	 * @param int $country_tier_id Country Tier Id
	 */
	public function SetCountryTierId($country_tier_id)
	{
		$this->country_tier_id = (int)$country_tier_id;

		$this->MarkDirty();
	}

	/**
	 * Sets the Account Id
	 * 
	 * @param int $account_id Account Id
	 */
	public function SetAccountId($account_id)
	{
		$this->account_id = (int)$account_id;

		$this->MarkDirty();
	}
}
?>