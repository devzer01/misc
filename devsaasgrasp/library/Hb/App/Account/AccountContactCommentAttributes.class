 * 
 * @copyright 2007 Global Market Insite Inc
 * @author Sujith T
 * @version 1.0
 * @package App 
 * @subpackage Account
 */  
class Hb_App_Account_AccountContactCommentAttributes extends Hb_App_Attributes 
{
  /**
   * Account Contact Comment Id
   *
   * @var int 
   */ 
   protected  $account_contact_comment_id = null;
   
  /**
   * Account Contact Comment domain object
   *
   * @var Hb_App_Account_AccountContactComment 
   */ 
   protected  $account_contact_comment = null;
   
  /**
	* Adds attributes into the collection
	*
	* @param Hb_App_Account_AccountContactCommentAttribute $attribute Object of Account Contact Comment Attribute
	* @throws Hb_Data_ObjectInCollectionException
	*/ 
	public function AddAttribute(Hb_App_Account_AccountContactCommentAttribute $attribute)
	{
		try {
			parent::AddAttribute($attribute);
		}  catch (Hb_Data_ObjectInCollectionException $e)  {
			throw new Hb_Data_ObjectInCollectionException("Attribute Already Exists In Collection For Account Contact Comment ". $this->account_contact_comment_id, 
				HB_DATA_EXCEPTION_ATTR_IN_COLLECTION);
		}
	}
	
  /**
	* Returns Hb_Data_Attribute object
	*
	* @param string $name Attribute Name
	* @return Hb_Data_Attribute
	* @throws Hb_Data_ObjectNotInCollectionException
	*/ 
	public function GetAttribute($name) 
	{
		try {
			return parent::GetAttribute($name);
		} catch (Hb_Data_ObjectNotInCollectionException $e) {
			throw new Hb_Data_ObjectNotInCollectionException("Requested Attribute Not Found For Account Contact Comment " . $this->account_contact_comment_id, 
				HB_DATA_EXCEPTION_ATTR_NOT_IN_COLLECTION);
		}
	}
	
  /**
	* Adds Account Contact Comment Attribute domain object
	*
	* @param Hb_App_Account_AccountContactCommentAttribute $account_contact_comment domain object 
	*/ 
	function AddAccountMerge(Hb_App_Account_AccountContactCommentAttribute $account_contact_comment) 
	{ 
		$this->account_contact_comment		= $account_contact_comment;
		$this->account_contact_comment_id	= $account_contact_comment->GetId();
	}
}
?>