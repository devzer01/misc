class Hb_App_Common_ConfigManager extends Hb_App_Object
{

	/**
     * Returns Authentication Setting Id
     *
     * @return int Authentication Setting Id
     */
	public static function GetAuthConfig($domain_name)
	{
		$authenticate_setting = Hb_App_ObjectHelper::GetMapper('Hb_App_Common_AuthenticateSetting')->FindByName($domain_name);	    	

		return $authenticate_setting;
	}
	
	
}
