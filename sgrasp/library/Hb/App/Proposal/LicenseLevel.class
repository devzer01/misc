class Hb_App_Proposal_LicenseLevel extends Hb_App_Object
{
	/**
	 * License Level Id
	 *
	 * @var int
	 */
	protected $license_level_id = null;
	
	/**
	 * License Level Description
	 *
	 * @var string
	 */
	protected $license_level_description = null;

	/**
	 * Create LicenseLevel Object
	 * 
	 * @param int $id License Level Id
	 * @param string $license_level_description License Level Description
	 */
	function __construct($id 								= null, 
								$license_level_description	= null
								)
	{
		$this->license_level_id				= $id;
		$this->license_level_description	= $license_level_description;

		parent::__construct($id);
	}

	/**
	 * Returns the License Level Id
	 *
	 * @return int License Level Id
	 */
	public function GetLicenseLevelId()
	{
		return $this->license_level_id;
	}
	
	/**
	 * Returns the LicenseLevel Description
	 *
	 * @return string LicenseLevel Description
	 */
	public function GetLicenseLevelDescription()
	{
		return $this->license_level_description;
	}

	/**
	 * Sets the License Level Id
	 * 
	 * @param int $license_level_id License Level Id
	 */
	public function SetLicenseLevelId($license_level_id)
	{
		$this->license_level_id = $license_level_id;
		
		$this->__id = $license_level_id;
		
		$this->MarkDirty();
	}
	
	/**
	 * Sets the License Level Description
	 * 
	 * @param string $license_level_description License Level Description
	 */
	public function SetLicenseLevelDescription($license_level_description)
	{
		$this->license_level_description = $license_level_description;

		$this->MarkDirty();
	}
}
?>