class Hb_App_Billing_BillingReportGroupAttributes extends Hb_App_Attributes 
{
	/**
	 * Billing Report instance
	 *
	 * @var HB_App_Billing_BillingReportGroup 
	 */
	protected $billing_report_group = null;
	
	/**
	 * Billing Report Id
	 *
	 * @var int 
	 */
	protected $billing_report_group_id	= null;
	
	public function AddAttribute(Hb_App_Billing_BillingReportGroupAttribute $attribute) 
	{
		try {
			parent::AddAttribute($attribute);
		} catch (Hb_Data_ObjectInCollectionException $e) {
			throw new Hb_Data_ObjectInCollectionException("Attribute Already Exists In Collection For Billing Report Group ". $this->billing_report_group_id,
				HB_DATA_EXCEPTION_ATTR_IN_COLLECTION);
		}
	}
	
	public function GetAttribute($name)
	{
		try {
			return parent::GetAttribute($name);
		}catch (Hb_Data_ObjectNotInCollectionException $e) {
			throw new Hb_Data_ObjectNotInCollectionException("Requested Attribute Not Found For Billing Report Group" . $this->billing_report_group_id,
				HB_DATA_EXCEPTION_ATTR_NOT_IN_COLLECTION);
		}
	}
	
	public function AddBillingReportGroup(Hb_App_Billing_BillingReportGroup $billing_report_group)
	{
		$this->billing_report_group		= $billing_report_group;
		$this->billing_report_group_id	= $billing_report_group->GetBillingReportGroupId();
	}
	
}

