class Hb_App_Billing_BillingReportAction extends Hb_App_Object
{
	/**
	 * Billing Report Action Id
	 *
	 * @var ini
	 */
	protected $armc_action_id = null;
	
	/**
	 * Billing Report Id
	 *
	 * @var int
	 */
	protected $armc_id = null;
	
	/**
	 * Billing Report Action Type Id
	 *
	 * @var int
	 */
	protected $armc_action_type_id = null;

	/**
	 * Create BillingReportAction Object
	 * 
	 * @param int $id Billing Report Action Id
	 * @param int $armc_id Billing Report Id
	 * @param int $armc_action_type_id Billing Report Action Type Id
	 */
	function __construct($id 						= null, 
								$armc_id					= null,							
								$armc_action_type_id	= null
								)
	{
		$this->armc_action_id		= $id;
		$this->armc_id 				= $armc_id;
		$this->armc_action_type_id	= $armc_action_type_id;
		
		parent::__construct($id);
	}
	
	/**
	 * Returns the Billing Report Action Id
	 *
	 * @return int Billing Report Action Id
	 */
	public function GetBillingReportActionId()
	{
		return $this->armc_action_id;
	}
	
	/**
	 * Returns the Billing Report Id
	 *
	 * @return int Billing Report Id
	 */
	public function GetBillingReportId()
	{
		return $this->armc_id;
	}
	
	/**
	 * Returns the Billing Report Action Type Id
	 *
	 * @return int Billing Report Action Type Id
	 */
	public function GetBillingReportActionTypeId()
	{
		return $this->armc_action_type_id;
	}

	/**
	 * Sets the Billing Report Action Id
	 * 
	 * @param int $armc_action_id Billing Report Action Id
	 */
	public function SetBillingReportActionId($armc_action_id)
	{
		$this->armc_action_id = $armc_action_id;
		
		$this->__id = $armc_action_id;

		$this->MarkDirty();
	}

	/**
	 * Sets the Billing Report Id
	 * 
	 * @param int $armc_id Billing Report Id
	 */
	public function SetBillingReportId($armc_id)
	{
		$this->armc_id = $armc_id;

		$this->MarkDirty();
	}
	
	/**
	 * Sets the Billing Report Action Type Id
	 * 
	 * @param int $armc_action_type_id Billing Report Action Type Id
	 */
	public function SetBillingReportActionTypeId($armc_action_type_id)
	{
		$this->armc_action_type_id = $armc_action_type_id;

		$this->MarkDirty();
	}
}
?>