class Hb_App_Billing_BillingReportAttributes extends Hb_App_Attributes 
{
	/**
	 * Billing Report instance
	 *
	 * @var HB_App_Billing_BillingReport 
	 */
	protected $billing_report = null;
	
	/**
	 * Billing Report Id
	 *
	 * @var int 
	 */
	protected $billing_report_id	= null;
	
	public function AddAttribute(Hb_App_Billing_BillingReportAttribute $attribute) 
	{
		try {
			parent::AddAttribute($attribute);
		} catch (Hb_Data_ObjectInCollectionException $e) {
			throw new Hb_Data_ObjectInCollectionException("Attribute Already Exists In Collection For Billing Report ". $this->billing_report_id,
				HB_DATA_EXCEPTION_ATTR_IN_COLLECTION);
		}
	}
	
	public function GetAttribute($name)
	{
		try {
			return parent::GetAttribute($name);
		}catch (Hb_Data_ObjectNotInCollectionException $e) {
			throw new Hb_Data_ObjectNotInCollectionException("Requested Attribute Not Found For Billing Report " . $this->billing_report_id,
				HB_DATA_EXCEPTION_ATTR_NOT_IN_COLLECTION);
		}
	}
	
	public function AddBillingReport(Hb_App_Billing_BillingReport $billing_report)
	{
		$this->billing_report		= $billing_report;
		$this->billing_report_id	= $billing_report->GetBillingReportId();
	}
	
}

