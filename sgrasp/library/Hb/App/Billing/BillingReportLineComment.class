class Hb_App_Billing_BillingReportLineComment extends Hb_App_Object 
{
	/**
	 * Billing Report Line Comment Id
	 * 
	 * @var int
	 */
	protected $billing_report_line_comment_id = null;

	/**
	 * Billing  Report Line Id
	 * 
	 * @var int
	 */
	protected $billing_report_line_id = null;
	
	/**
	 * Billing  Report Comment Type Id
	 * 
	 * @var int
	 */
	protected $billing_report_comment_type_id = null;

	/**
	 * Billing  Report Comment
	 * 
	 * @var string
	 */
	protected $billing_report_comment = null;

	
	/**
	* Contructor of the Billing Report Line Comment Domain
	* 
	* @param int $id Billing Report Line Comment Id
	* @param int $billing_report_line_id Billing  Report Line Id
	* @param string $billing_report_comment_type_id Billing  Report Comment Type Id
	* @param int $billing_report_comment Billing  Report Comment
	*/		
	function __construct($id 				                 = null,	
							   $billing_report_line_id         = null,
							   $billing_report_comment_type_id = null,
							   $billing_report_comment         = null
							  )
	{		 					
		
		$this->billing_report_line_comment_id = $id;
		$this->billing_report_line_id         = $billing_report_line_id;
		$this->billing_report_comment_type_id = $billing_report_comment_type_id ;
		$this->billing_report_comment 	     = $billing_report_comment;		
				
		parent::__construct($id);
	}
	
	
	/**
	 * Get Billing Report Line Comment Id
	 *	
	 * @return int $billing_report_line_comment_id Billing Report Line Comment Id
	 */
	public function GetBillingReportLineCommentId()
	{
		return $this->billing_report_line_comment_id;
	}

	
	/**
	 * Get Billing  Report Line Id
	 *	
	 * @return int $billing_report_line_id Billing  Report Line Id
	 */
	public function GetBillingReportLineId()	
	{
		return $this->billing_report_line_id;
	}
	
	
	/**	
	 * Get Billing  Report Comment Type Id
	 *
	 * @return int $billing_report_comment_type_id Billing  Report Comment Type Id	
	 */
	public function GetReportCommentTypeId()
	{
		return $this->billing_report_comment_type_id;
	}
	
	/**	
	 * Get Billing  Report Comment
	 *
	 * @return string $billing_report_comment Billing  Report Comment
	 */
	public function GetBillingReportComment()
	{
		return $this->billing_report_comment;
	}
	
	
	/**
	 * Sets the Billing Report Line Comment Id
	 *
	 * @param int $billing_report_line_comment_id Billing Report Line Comment Id
	 */
	public function SetBillingReportLineCommentId($billing_report_line_comment_id)
	{
		$this->billing_report_line_comment_id = $billing_report_line_comment_id;

		$this->__id = $billing_report_line_comment_id ;	
		
		$this->MarkDirty();
	}

	
	/**
	 * Sets Billing  Report Line Id
	 *
	 * @param int $billing_report_line_id Billing  Report Line Id
	 */
	public function SetBillingReportLineId($billing_report_line_id)
	{
		$this->billing_report_line_id = $billing_report_line_id;
		
		$this->MarkDirty();
	}

	
	/**
	 * Sets Billing  Report Comment Type Id
	 * 
	 * @param int $billing_report_comment_type_id Billing  Report Comment Type Id
	 */
	public function SetReportCommentTypeId($billing_report_comment_type_id)
	{
		$this->billing_report_comment_type_id = $billing_report_comment_type_id;
		
		$this->MarkDirty();
	}	
	
	/**
	 * Sets Billing  Report Comment
	 * 
	 * @param string $billing_report_comment Billing  Report Comment
	 */
	public function SetBillingReportComment($billing_report_comment)
	{
		$this->billing_report_comment = $billing_report_comment;
		
		$this->MarkDirty();
	}
}	
