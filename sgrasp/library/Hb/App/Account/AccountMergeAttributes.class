 * 
 * @copyright 2007 Global Market Insite Inc
 * @author Administrator
 * @version 1.0
 * @package App 
 * @subpackage Account
 */  
class Hb_App_Account_AccountMergeAttributes extends Hb_App_Attributes 
{
   /**
    * Account Merge Id
    *
    * @var int 
    */ 
    protected $account_merge_id = null;
    
   /**
    * Account Merge domain object
    *
    * @var Hb_App_Account_AccountMerge
    */ 
    protected $account_merge = null;
    
	/**
	 * Adds attributes into the collection
	 *
	 * @param Hb_App_Account_AccountMergeAttribute $attribute Object of Account Merge Attribute
	 * @throws Hb_Data_ObjectInCollectionException
	 */ 
	public function AddAttribute(Hb_App_Account_AccountMergeAttribute $attribute)
	{
		try {
			parent::AddAttribute($attribute);
		}  catch (Hb_Data_ObjectInCollectionException $e)  {
			throw new Hb_Data_ObjectInCollectionException("Attribute Already Exists In Collection For Account Merge ". $this->account_merge_id, 
				HB_DATA_EXCEPTION_ATTR_IN_COLLECTION);
		}
	}
	
	/**
	 * Returns Hb_Data_Attribute object
	 *
	 * @param string $name Attribute Name
	 * @return Hb_Data_Attribute
	 * @throws Hb_Data_ObjectNotInCollectionException
	 */ 
	public function GetAttribute($name)
	{
		try {
			return parent::GetAttribute($name);
		} catch (Hb_Data_ObjectNotInCollectionException $e) {
			throw new Hb_Data_ObjectNotInCollectionException("Requested Attribute Not Found For Account Megre " . $this->account_merge_id, 
				HB_DATA_EXCEPTION_ATTR_NOT_IN_COLLECTION);
		}
	}
	
	/**
	 * Adds Account Merge domain object
	 *
	 * @param Hb_App_Account_AccountMerge $account_merge domain object 
	 */ 
	function AddAccountMerge(Hb_App_Account_AccountMerge $account_merge) 
	{ 
		$this->account_merge		= $account_merge;
		$this->account_merge_id		= $account_merge->GetId();
	}
} 
?>