class Hb_App_Account_AccountPanelCost extends Hb_App_Object
{

	/**
	 * Account id
	 *
	 * @var int
	 */
	protected $account_id = null;
	
	/**
	 * Account Panel Cost Id
	 *
	 * @var int
	 */
	protected $account_panel_cost_id = null;
	
	/**
	 * Effective Date
	 *
	 * @var string
	 */
	protected $effective_date = null;
	
	/**
	 * Expier Date
	 *
	 * @var string
	 */
	protected $expire_date = null;
	
	/**
	 * Pricing Regime Id
	 *
	 * @var int
	 */
	protected $pricing_regime_id = null;
	/**
	 * Construct an Account Panel Cost object
	 *
	 * @param int $id  Account Panel Cost Id
	 * @param int $pricing_regime_id Priceing Regime Id
	 * @param int $account_id Account Id
	 * @param string $effective_date Effective Date
	 * @param string $expire_date Expire Date
	 */
	function __construct(	$id = null,
							$pricing_regime_id = null,
							$account_id = null,
							$effective_date = null,
							$expire_date = null
						)
	{
		$this->account_panel_cost_id = $id;
		$this->pricing_regime_id = $pricing_regime_id;
		$this->account_id = $account_id;
		$this->effective_date = $effective_date;
		$this->expire_date = $expire_date;
		
		parent::__construct($id);
	}

	/**
	* Returns The Account Id
 	* 
	* @return int The Account Id
	**/
	public function GetAccountId()
	{
		return $this->account_id;
	}

	/**
	 * Returns The Account Panel Cost Id
	 *
	 * @return int The Account Panel Cost Id
	 */
	public function GetAccountPanelCostId()
	{
		return $this->account_panel_cost_id;
	}
	
	/**
	 * Returns The Effective Date
	 *
	 * @return string The Effective Date
	 */
	public function GetEffectiveDate()
	{
		return $this->effective_date;
	}

	/**
	 * Returns The Expire Date
	 *
	 * @return string  The Expire Date
	 */
	public function GetExpireDate()
	{
		return $this->expire_date;
	}

	/**
	 * Returns The Pricing Regime Id
	 *
	 * @return int The Pricing Regime Id
	 */
	public function GetPricingRegimeId()
	{
		return $this->pricing_regime_id;
	}

	/**
	 * Sets The Account Id
	 * 
	 * @param int $account_id The Account Id
	 */
	public function SetAccountId($account_id)
	{
		$this->account_id = (int) $account_id;

		$this->MarkDirty();
	}

	/**
	 * Sets The Account Panel Cost Id
	 * 
	 * @param int $account_panel_cost_id The Account Panel Cost Id
	 */
	public function SetAccountPanelCostId($account_panel_cost_id)
	{
		$this->account_panel_cost_id = (int) $account_panel_cost_id;
		$this->__id = (int) $account_panel_cost_id;
		
		$this->MarkDirty();
		
	}

	/**
	 * Sets The Effective Date
	 * 
	 * @param string $effective_date The Effective Date
	 */
	public function SetEffectiveDate($effective_date)
	{
		$this->effective_date = $effective_date;

		$this->MarkDirty();
	}

	/**
	 * Sets The Expire Date
	 * 
	 * @param string $expire_date The Expire Date
	 */
	public function SetExpireDate($expire_date)
	{
		$this->expire_date = $expire_date;

		$this->MarkDirty();
	}

	/**
	 * Sets The Pricing Regime Id
	 * 
	 * @param int $pricing_regime_id The Pricing Regime Id
	 */
	public function SetPricingRegimeId($pricing_regime_id)
	{
		$this->pricing_regime_id = (int) $pricing_regime_id;

		$this->MarkDirty();
	}
	
}
?>