class Hb_App_Account_AccountPricingItemGroupDiscount extends Hb_App_Object
{

	/**
	 * Account pricing item group dicount amount
	 *
	 * @var double
	 **/
	protected $discount_amount = null;

	/**
	 * Account pricing item group discount effective date
	 *
	 * @var string
	 **/	
	protected $effective_date = null;

	/**
	 * Account pricing item group discount expire date
	 *
	 * @var string
	 **/	
	protected $expire_date = null;

	/**
	 * Account Id
	 *
	 * @var int
	 **/	
	protected $account_id = null;

	/**
	 * Account pricing item group discount Id
	 *
	 * @var int
	 **/
	protected $account_pricing_item_group_discount_id = null;

	/**
	 * Pricing discount type Id
	 *
	 * @var int
	 **/
	protected $pricing_discount_type_id = null;

	/**
	 * Pricing item group Id
	 *
	 * @var int
	 **/
	protected $pricing_item_group_id = null;

	/**
	 * Create AccountPricingItemGroupDiscount Object
	 *
	 * @param int $id Account pricing item group discount Id
	 * @param int $pricing_item_group_id Pricing item group Id
	 * @param int $account_id Account Id
	 * @param int $pricing_discount_type_id Pricing discount type Id
	 * @param int $discount_amount Discount amount
	 * @param int $effective_date Effective date
	 * @param int $expire_date Expire Date
	 */
	function __construct($id = null,
  							   $pricing_item_group_id = null,
  							   $account_id = null, 
  							   $pricing_discount_type_id = null,
  							   $discount_amount = null,
  							   $effective_date = null, 
  							   $expire_date = null )
	{
		$this->account_pricing_item_group_discount_id = $id;
		$this->pricing_item_group_id = $pricing_item_group_id;
		$this->account_id = $account_id;
		$this->pricing_discount_type_id = $pricing_discount_type_id;
		$this->discount_amount = $discount_amount;
		$this->effective_date = $effective_date;
		$this->expire_date = $expire_date;
		parent::__construct($id);
	}

	/**
	* Returns the Discount amount
 	* 
	* @return double The Discount amount
	**/
	public function GetDiscountAmount()
	{
		return $this->discount_amount;
	}

	/**
	* Returns the Effective date
 	* 
	* @return string The Effective date
	**/
	public function GetEffectiveDate()
	{
		return $this->effective_date;
	}

	/**
	* Returns the Expire date
 	* 
	* @return string The Expire date
	**/
	public function GetExpireDate()
	{
		return $this->expire_date;
	}

	/**
	* Returns the Account Id
 	* 
	* @return int The Account Id
	**/
	public function GetAccountId()
	{
		return $this->account_id;
	}

	/**
	* Returns the Account pricing item group discount Id
 	* 
	* @return int The Account pricing item group discount Id
	**/
	public function GetAccountPricingItemGroupDiscountId()
	{
		return $this->account_pricing_item_group_discount_id;
	}

	/**
	* Returns the Pricing discount type Id
 	* 
	* @return int The Pricing discount type Id
	**/
	public function GetPricingDiscountTypeId()
	{
		return $this->pricing_discount_type_id;
	}

	/**
	* Returns the Pricing item group Id
 	* 
	* @return int The Pricing item group Id
	**/
	public function GetPricingItemGroupId()
	{
		return $this->pricing_item_group_id;
	}

	/**
	 * Sets the Discount amount
 	 * 
	 * @param double $discount_amount Discount amount
	 */
	public function SetDiscountAmount($discount_amount)
	{
		$this->discount_amount = (double) $discount_amount;
		$this->MarkDirty();
	}

	/**
	 * Sets the Effective date
 	 * 
	 * @param string $effective_date Effective date
	 */
	public function SetEffectiveDate($effective_date)
	{
		$this->effective_date = $effective_date;
		$this->MarkDirty();
	}

	/**
	 * Sets the Expire date
 	 * 
	 * @param string $expire_date Expire date
	 */
	public function SetExpireDate($expire_date)
	{
		$this->expire_date = $expire_date;
		$this->MarkDirty();
	}

	/**
	 * Sets the Account Id
 	 * 
	 * @param int $expire_date Expire date
	 */
	public function SetAccountId($account_id)
	{
		$this->account_id = (int) $account_id;
		$this->MarkDirty();
	}

	/**
	 * Sets the Account pricing item group discount Id
 	 * 
	 * @param int $account_pricing_item_group_discount_id Account pricing item group discount Id
	 */
	public function SetAccountPricingItemGroupDiscountId($account_pricing_item_group_discount_id)
	{
		$this->account_pricing_item_group_discount_id = $account_pricing_item_group_discount_id;
		$this->__id = (int) $account_pricing_item_group_discount_id;
		$this->MarkDirty();		
	}

	/**
	 * Sets the Pricing discount type Id
 	 * 
	 * @param int $pricing_discount_type_id Pricing discount type Id
	 */
	public function SetPricingDiscountTypeId($pricing_discount_type_id)
	{
		$this->pricing_discount_type_id = (int) $pricing_discount_type_id;
		$this->MarkDirty();		
	}

	/**
	 * Sets the Pricing item group Id
 	 * 
	 * @param int $pricing_item_group_id Pricing item group Id
	 */
	public function SetPricingItemGroupId($pricing_item_group_id)
	{
		$this->pricing_item_group_id = (int) $pricing_item_group_id;
		$this->MarkDirty();		
	}

}
