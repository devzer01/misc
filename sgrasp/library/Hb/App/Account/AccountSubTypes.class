class Hb_App_Account_AccountSubTypes extends Hb_App_Collection 
{
	/**
	 * Account Type Id
	 *
	 * @var int 
	 */ 
	protected $account_type_id = null;
	
	/**
	 * Add Account Sub Types
	 *
	 * @param Hb_App_Account_AccountSubType $subtype Account Sub Type
	 */ 
	public function AddItem(Hb_App_Account_AccountSubType $subtype)
	{
		$this->account_type_id = $subtype->GetAccountTypeId();
		parent::AddItem($subtype);
	}
}
