class Hb_App_Study_StudyTimeLineAttributeDefinition extends Hb_App_Common_AttributeDefinition
{

	/**
	 * Attribute Type
	 *
	 * @var string
	 */   	
	protected $attribute_type = null;	

	/**
	 * Constructor of Domain Class
	 *
	 * @param int $id Study Time Line Attribute Definition Id
	 * @param string $attribute_name Study Time Line Attribute Definition Name
	 * @param string $description Study Time Line Attribute Definition Description
	 * @param string $attribute_type Study Time Line Attribute Type
	 */ 
	function __construct($id = null, 
								$attribute_name = null, 
								$description = null,
								$attribute_type = null) 
	{ 
		$this->attribute_type = $attribute_type;
		parent::__construct($id, $attribute_name, $description);
	}
	
	/**
	 * Return Attribute type
	 *
	 * @return string Attribute type
	 */	
	public function GetAttributeType()
	{
		return $this->attribute_type;		
	}

	/**
	 * Sets Attribute Type
	 *
	 * @param string $attribute_type Attribute type
	 */	
	public function SetAttributeType($attribute_type)
	{
		$this->attribute_type = $attribute_type;		
		$this->MarkDirty();
	}

}
