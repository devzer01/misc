	}

	/**
	 * Set the Alert email for the  STudy TimeLine Alert
	 * 
	 * @param int $alert_email Alert email
	 */
	public function SetAlertEmail($alert_email)
	{
		$this->alert_email = $alert_email;
		$this->MarkDirty();
	}

	/**
	 * Set the Alert jabber for the  STudy TimeLine Alert
	 * 
	 * @param int $alert_jabber Alert jabber
	 */
	public function SetAlertJabber($alert_jabber)
