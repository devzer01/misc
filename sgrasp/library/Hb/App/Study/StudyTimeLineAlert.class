	 *
	 * @param int $id study timeline id
	 * @param int $alert_email Alert email
	 * @param int $alert_jabber Alert jabber
	 * @param int $alert_mobile Alert mobile
	 * @param string $alert_sent_date Alert sent date
	 * @param int $login Login
	 * @param int $study_timeline_id Study timeline id
	 */
	function __construct($id 						= null,
								$alert_email 			= null,
								$alert_jabber 			= null,
								$alert_mobile 			= null,
								$alert_sent_date 		= null,
								$login 					= null,
								$study_timeline_id 	= null
								)
	{
		$this->study_timeline_alert_id 	= $id;
		$this->study_timeline_id			= $study_timeline_id;
		$this->alert_email					= $alert_email;
		$this->alert_jabber					= $alert_jabber;
		$this->alert_mobile					= $alert_mobile;
		$this->alert_sent_date				= $alert_sent_date;
		$this->login							= $login;
		
		parent::__construct($id);
	}

	/**
	 * Return the Alert email for the STudy TimeLine Alert
	 *
	 * @return int Alert email
	 */
	public function GetAlertEmail()
	{
		return $this->alert_email;
	}

	/**
	 * Return the Alert Jabber for the STudy TimeLine Alert
	 *
	 * @return int Alert Jabber
	 */
	public function GetAlertJabber()
	{
