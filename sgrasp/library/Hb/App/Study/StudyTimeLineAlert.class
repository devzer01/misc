	}

	/**
	 * Return the Login for the STudy TimeLine Alert
	 *
	 * @return int Login
	 */
	public function GetLogin()
	{
		return $this->login;
	}

	/**
	 * Return the Study time line alert id for the STudy TimeLine Alert
	 *
	 * @return int Study time line alert id
